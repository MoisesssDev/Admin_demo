
<% if !user_signed_in? %>
  <div class="container-img">
    <%= image_tag 'logo.png' %>
  </div>
  <div class="container-btn">
    <%= link_to 'Sign in', new_user_session_path, class: 'btn-2' %>
    <%= link_to 'Sign up', new_user_registration_path, class: 'btn' %>
  </div>

<% else %>
  <h1>Welcome to Admin_Demo</h1>

  <div class="container-btn">
    <%= link_to 'Adminstrate', admin_products_path, class: 'btn' %>

    <div class="container-products">
    <%= render @products %>
</div>
  </div>
<% end %>
